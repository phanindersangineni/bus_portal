<div id="mydiv">
    <img src="images/spinner.gif" class="ajax-loader"/>
</div>

<div class="page-title-container">
            <div class="container">
                <div class="page-title pull-left">
                    <h2 class="entry-title" style="font-size: 0.37em; !important" ng-bind="thanksLabel">Thank You</h2>
                </div>
                <ul class="breadcrumbs pull-right">
                    <li><a ng-click="nagivatehome()" ng-bind="hometLabel">HOME</a></li>
                    <li class="active" ng-bind="tktLabel">Ticket Booking</li>
                </ul>
            </div>
        </div>


        <section id="content" class="gray-area">
            <div class="container">
                <div class="row"><br>
                    <div class="col-sm-8 col-md-9">
                        <div class="booking-information travelo-box">
                            <h2 ng-bind="bkngcnfrmLabel">Booking Confirmation</h2>
                            <hr />
                            <div class="booking-confirmation clearfix">
                                <i class="soap-icon-recommend icon circle"></i>
                                <div class="message" ng-if="canshow">
                                    <h4 class="main-message" ng-bind="cnfmmmLabel">Thank You. Your Ticket booking is Confirmed Now.</h4>
                                    <p ng-bind="adrsLabel">A confirmation email has been sent to your provided email address.</p>
                                </div>
                                  <br><br><br>
								  <a href="#ref-conf"  ng-if="bookingdetails[0].BOOKING_STATUS=='PENDING'" class="print-button button btn-small blue soap-popupbox" ng-bind="confrefLabel">Add Booking Reference</a>
								  <a href="#ref-con"  class="print-button button btn-small green soap-popupbox">Send Mail</a>
                                   </p>
                                <a href="#printticket" class="print-button button btn-small yellow" ng-bind="prntLabel">PRINT DETAILS</a>
                            </div>
                            <hr />

                            <h2 ng-bind="travelinfoLabel">Traveler Information</h2>
                              <dl class="term-description">
                                <dt ng-bind="bkngnumLabel">Booking number:</dt><dd>{{bookingdetails[0].BOOKING_ID}}</dd>
                                <dt ng-bind="bkngstatusLabel">Booking status:</dt><dd><b>{{bookingdetails[0].BOOKING_STATUS}}</b></dd>
                                 <dt ng-bind="bkngdateLabel">Booking Date:</dt><dd>{{bookingdetails[0].BKDATE}}</dd>
                                <dt ng-bind="usernameLabel">User name:</dt><dd>{{bookingdetails[0].CUSTOMERNAME}}</dd>
                                <dt ng-bind="emailLabel">E-mail address:</dt><dd>{{bookingdetails[0].EMAIL_ID}}</dd>
                                <dt ng-bind="streetLabel">Street Address and number:</dt><dd>{{bookingdetails[0].BOOKINGADDRESS}}</dd>
                                <dt ng-bind="documentLabel" ng-if="bookingdetails[0].DOCUMENT !="-1"">Document:</dt><dd ng-if="bookingdetails[0].DOCUMENT !="-1"">{{bookingdetails[0].DOCUMENT}}</dd>
                                <dt ng-bind="documentnoLabel" ng-if="bookingdetails[0].DOCUMENT_NUMBER !=null">Document Number:</dt><dd>{{bookingdetails[0].DOCUMENT_NUMBER}}</dd>
                                <dt ng-bind="phoneLabel">Phone:</dt><dd>+{{bookingdetails[0].COUNTRY_CODE}} {{bookingdetails[0].PHONE}} </dd>

                            </dl>
                            <hr />

                             <h2 ng-bind="tktinfoLabel">Ticket Information</h2>
                              <hr>
                               <div class="form-group row">
                                 <div class="col-sm-6 col-md-5">
                                            <label style="color:#01b7f2;" >{{routeLabel}} : </label>
                                         <!-- <p ng-bind="departlabel">Departure from {{bookingdetails[0].FROMCITYNAME}} to {{bookingdetails[0].TOCITYNAME}}</p>-->
										  <p >{{departlabel}} {{bookingdetails[0].FROMCITYNAME}} {{toLabel}} {{bookingdetails[0].TOCITYNAME}}</p>
                                        </div>
                               </div>

                               <div class="form-group row">
                                        <div class="col-sm-6 col-md-2">
                                            <label style="color:#01b7f2;" ng-bind="tktnoLabel">Ticket Number :</label>
                                          <p> {{ticketdetails[0].TICKET_ID}} </p>
                                        </div>
                                        <div class="col-sm-6 col-md-2">
                                            <label style="color:#01b7f2;" ng-bind="depttimeLabel">Departure Time:</label>
                                          <p> {{ticketdetails[0].DEPARTURE_TIME}} <p>{{ticketdetails[0].DEPARTURE_DATE}} </p> 
                                        </div>
                                         <div class="col-sm-6 col-md-2">
                                            <label style="color:#01b7f2;" ng-bind="arvltimeLabel">Arrival Time:</label>
                                            <p>{{ticketdetails[0].ARRIVAL_TIME}} <p>{{ticketdetails[0].ARRIVAL_DATE}}  </p>
                                        </div>
                                         <div class="col-sm-6 col-md-2"  ng-if="ticketdetails[0].SEAT_NUMBER ==0">
                                            <label style="color:#01b7f2;" ng-bind="cnfrmseatsLabel">Confirmed Seats:</label>
                                            <p> {{ticketdetails[0].TOTAL_TICKETS}}</p>
                                        </div>
                                        <div class="col-sm-3 col-md-2" ng-if="ticketdetails[0].SEAT_NUMBER !=0" >
                                            <label style="color:#01b7f2;">Seat Numbers</label>
                                            <p> {{ticketdetails[0].SEAT_NUMBER}}</p>
                                        </div>
                                         <div class="col-sm-6 col-md-2">
                                            <label style="color:#01b7f2;" ng-bind="traveldateLabel">Travel Date:</label>
                                           <p> {{ticketdetails[0].TRAVELDATE}}</p>
                                        </div>
                                         <div class="col-sm-6 col-md-2">
                                            <label style="color:#01b7f2;" ng-bind="vehLabel">Vehicle Number:</label>
                                           <p> {{ticketdetails[0].VEH_REG_NUMBER}}</p>
                                        </div>
                                    </div>

                                   <h2 id="pickupdropLabel">Your Pick and Drop points</h2>

                                    <div class="form-group row">
                                        <div class="col-sm-6 col-md-3">
                                            <label id="pickupLabel">Pickup Point</label>
                                            <div >
                                             {{ticketdetails[0].PICKUP_POINT_NAME}}
                                            </div>
                                        </div>
                                         <div class="col-sm-6 col-md-3">
                                            <label id="dropLabel">Drop Point</label>
                                            <div >
                                                {{ticketdetails[0].DROP_POINT_NAME}}
                                            </div>
                                        </div>
                                        
                                    </div>




                                    <div class="form-group row" ng-if ="ticketdetails.length==2">
                                        <div class="col-sm-6 col-md-2">
                                            <label style="color:#01b7f2;">Ticket Number : </label>
                                           <p>{{ticketdetails[1].TICKET_ID}} </p>
                                        </div>
                                        <div class="col-sm-6 col-md-2">
                                            <label style="color:#01b7f2;">Departure Time:</label>
                                           <p>{{ticketdetails[1].DEPARTURE_TIME}} <p>{{ticketdetails[1].DEPARTURE_DATE}} </p> 
                                        </div>
                                         <div class="col-sm-6 col-md-2">
                                            <label style="color:#01b7f2;">Arrival Time:</label>
                                            <p>{{ticketdetails[1].ARRIVAL_TIME}} <p>{{ticketdetails[1].ARRIVAL_DATE}} </p> 
                                        </div>
                                         <div class="col-sm-6 col-md-2" ng-if="ticketdetails[1].SEAT_NUMBER ==0">
                                            <label style="color:#01b7f2;">Confirmed Seats:</label>
                                            <p> {{ticketdetails[1].TOTAL_TICKETS}}</p>
                                        </div>
                                          <div class="col-sm-3 col-md-2" ng-if="ticketdetails[1].SEAT_NUMBER !=0">
                                            <label style="color:#01b7f2;">Seat Numbers</label>
                                            <p> {{ticketdetails[1].SEAT_NUMBER}}</p>
                                        </div>
                                         <div class="col-sm-6 col-md-2">
                                            <label style="color:#01b7f2;">Travel Date:</label>
                                            <p>{{ticketdetails[1].TRAVELDATE}}</p>
                                        </div>
                                         <div class="col-sm-6 col-md-2">
                                            <label style="color:#01b7f2;" >Vehicle Number:</label>
                                           <p> {{ticketdetails[1].VEH_REG_NUMBER}}</p>
                                        </div>
                                    </div>

                                    <div  ng-if ="ticketdetails.length==2">
                                       
                                     <h2 id="pickupdropLabel">Your Pick and Drop points</h2>

                                    <div class="form-group row">
                                        <div class="col-sm-6 col-md-3">
                                            <label id="pickupLabel">Pickup Point</label>
                                            <div >
                                             {{ticketdetails[1].PICKUP_POINT_NAME}}
                                            </div>
                                        </div>
                                         <div class="col-sm-6 col-md-3">
                                            <label id="dropLabel">Drop Point</label>
                                            <div >
                                                {{ticketdetails[1].DROP_POINT_NAME}}
                                            </div>
                                        </div>
                                        
                                    </div>
                                    </DIV>

                              </hr>

                            <p class="red-color" ng-bind="payLabel">Payment is made by Credit Card Via Paypal.</p>
                            <hr />
                          
                        </div>
                    </div>
                   <div class="sidebar col-sms-6 col-sm-4 col-md-3">
                        <div class="booking-details travelo-box">
                            <h4 ng-bind="onewayLabel">Booking Oneway Trip</h4>
                           

                           <article class="flight-booking-details">
                                <figure class="clearfix">
                                     <div class="travel-title">
                                        <h5 class="box-title">  {{ticketdetails[0].COMPANY_NAME}}</h5>

                                        <h5 class="box-title" ng-if="ticketdetails[0].PROMO_CODE ==null"  style="margin-left:-90px;">{{ticketdetails[0].TOTAL_TICKETS}} {{passengersLabel}}  $ {{ticketdetails[0].TOTAL_PRICE}}</h5>
                                       <h5 class="box-title" ng-if="ticketdetails[0].PROMO_CODE !=null"  style="margin-left:-90px;">{{ticketdetails[0].TOTAL_TICKETS}} {{passengersLabel}}  $ {{ticketdetails[0].TOTAL_PRICE- ticketdetails[0].PROMO_AMOUNT}}</h5>

                                       <h5 class="skin-color" ng-if="ticketdetails[0].PROMO_CODE !=null"  style="margin-left:-90px;"> PROMO CODE : {{ticketdetails[0].PROMO_CODE}}</h5>


                                    </div>
                                </figure>
                                
                                
                                <div class="details">
                                    <div class="icon-box style11 full-width">
                                        <div class="icon-wrapper">
                                            <i class="soap-icon-departure"></i>
                                        </div>
                                        <dl class="details">
                                            <dt class="skin-color" ng-bind="adultLabel">Adult</dt>
                                            <dd> {{ticketdetails[0].ADULT}}  *  $ {{ticketdetails[0].ADULT_TICKET_PRICE}}  </dd>
                                        </dl>
                                    </div>
                                    <div class="icon-box style11 full-width"  ng-if="childticketprice!=0">
                                        <div class="icon-wrapper">
                                            <i class="soap-icon-departure"></i>
                                        </div>
                                        <dl class="details">
                                            <dt class="skin-color" ng-bind="childrenLabel">Children</dt>
                                            <dd>  {{ticketdetails[0].CHILD}} * $ {{ticketdetails[0].CHILD_TICKET_PRICE}} </dd>
                                        </dl>
                                    </div>
                                    <div class="icon-box style11 full-width"  ng-if="retiredticketprice!=0">
                                        <div class="icon-wrapper">
                                            <i class="soap-icon-departure"></i>
                                        </div>
                                        <dl class="details">
                                            <dt class="skin-color" ng-bind="retiredLabel">Retired</dt>
                                            <dd> {{ticketdetails[0].RETIRED}} *  $ {{ticketdetails[0].RETIRED_TICKET_PRICE}}</dd>
                                        </dl>
                                    </div>
                                </div>
                               
                            </article>

                             
                            <article class="flight-booking-details" ng-if ="ticketdetails.length==2">
                                <figure class="clearfix">
                                   
                                    <div class="travel-title">
                                        <h5 class="box-title" style="margin-left:-90px;">{{ticketdetails[1].COMPANY_NAME}} (return trip)</h5>
                                       <h5 class="box-title" ng-if="ticketdetails[1].PROMO_CODE ==null"  style="margin-left:-90px;">{{ticketdetails[1].TOTAL_TICKETS}} {{passengersLabel}}  $ {{ticketdetails[1].TOTAL_PRICE}}</h5>
                                       <h5 class="box-title" ng-if="ticketdetails[1].PROMO_CODE !=null"  style="margin-left:-90px;">{{ticketdetails[1].TOTAL_TICKETS}} {{passengersLabel}}  $ {{ticketdetails[1].TOTAL_PRICE- ticketdetails[1].PROMO_AMOUNT}}</h5>

                                       <h5 class="skin-color" ng-if="ticketdetails[1].PROMO_CODE !=null"  style="margin-left:-90px;"> PROMO CODE : {{ticketdetails[1].PROMO_CODE}}</h5>
                                    </div>
                                </figure>

                                <div class="details">
                                    <div class="icon-box style11 full-width">
                                        <div class="icon-wrapper">
                                            <i class="soap-icon-departure"></i>
                                        </div>
                                       <dl class="details">
                                            <dt class="skin-color">Adult</dt>
                                            <dd> {{ticketdetails[1].ADULT}}  *  $ {{ticketdetails[1].ADULT_TICKET_PRICE}}  </dd>
                                        </dl>
                                    </div>
                                    <div class="icon-box style11 full-width"  ng-if="childticketprice!=0">
                                        <div class="icon-wrapper">
                                            <i class="soap-icon-departure"></i>
                                        </div>
                                        <dl class="details">
                                            <dt class="skin-color">Children</dt>
                                            <dd>  {{ticketdetails[1].CHILD}} * $ {{ticketdetails[1].CHILD_TICKET_PRICE}} </dd>
                                        </dl>
                                    </div>
                                    <div class="icon-box style11 full-width"  ng-if="retiredticketprice!=0">
                                        <div class="icon-wrapper">
                                            <i class="soap-icon-departure"></i>
                                        </div>
                                        <dl class="details">
                                            <dt class="skin-color">Retired</dt>
                                            <dd> {{ticketdetails[1].RETIRED}} *  $ {{ticketdetails[1].RETIRED_TICKET_PRICE}}</dd>
                                        </dl>
                                    </div>
                                </div>
                               
                            </article>

                            
                            <h4 ng-bind="taxLabel">Taxes & Fees</h4>
                            <dl class="other-details">
                                <dt class="feature" ng-bind="totalLabel">Total Seats:</dt><dd class="value">{{paymentdetails[0].TOTAL_SEATS}}</dd>
                                <dt class="feature" ng-bind="subLabel">Sub Total:</dt><dd class="value"> $ {{paymentdetails[0].TOTAL_PRICE}}</dd>
                                <dt class="feature" ng-bind="chargeLabel">Charges:</dt><dd class="value">$ {{paymentdetails[0].TOTAL_CHARGES}}</dd>
                                   
                                  <dt class="feature" ng-bind="taxesLabel">Taxes:</dt>
                                  <dd class="value" >$ {{paymentdetails[0].TOTAL_TAX}}

                                  <dt class="feature" ng-if="paymentdetails[0].TOTAL_DISCOUNT!=null" >Discounts:</dt>
                                  <dd class="value" >$ {{paymentdetails[0].TOTAL_DISCOUNT}}
                                  </dd> 
                                <dt class="total-price" ng-bind="priceLabel">Total Price(after discounts)</dt><dd class="total-price-value">$ {{paymentdetails[0].TRANSACTION_AMOUNT}}</dd>

                            </dl>
                        </div>
                        
                        <div class="travelo-box contact-box">
                          <h4 ng-bind="needLabel">Need Boletobus Help?</h4>
                           <p ng-bind="weLabel">We would be more than happy to help you. Our team advisor are 24/7 at your service to help you.</p>
                            <address class="contact-details">
                                <span class="contact-phone"><i class="soap-icon-phone"></i> +507 6148-2174</span>
                                <br>
                                <a class="contact-email" href="#">hola@bukate.com</a>
                            </address>
                        </div>
                    </div>
                </div>
            </div>
        </section>

        <div id="ref-conf" class="travelo-login-box travelo-box">
                
             
                <div class="seperator"><label>{{confrefLabel}} </label></div>
                <form>
                 {{refmobileno}}
                  
                    <div class="form-group">
                        <input type="text" ng-model="refid" id="refid" class="input-text full-width" placeholder="{{confrefLabel}}">
                       
                    </div>

                     <div class="form-group">
                        <input type="text" ng-model="guestnumber" id="guestnumber" class="input-text full-width" placeholder="{{telplaceholder}}" valid-number>
                       
                    </div>
                
                </form>
                <div class="seperator"></div>
            
                <p><button type="submit" ng-click="tagref()" class="btn-mediumlogin"> {{Continuelabel}}</button></p>
		
		<button type="button"  class="btn-mediumguest" onclick="jQuery('#soap-popupbox').css('display', 'none');">CLOSE</button>
                <div class="seperator"></div>
                
            </div>
			
			
			
			
			 <div id="ref-con" class="travelo-login-box travelo-box">
                
             
                <div class="seperator"><label>Send Mail </label></div>
                <form>
                 
                  
                    <div class="form-group">
                        <input type="text" ng-model="maildata" id="maildata" class="input-text full-width" placeholder="Enter Mail ID">
                       
                    </div>

                
                </form>
                <div class="seperator"></div>
            
                <p><button type="submit" ng-click="sendmail()" class="btn-mediumlogin"> SEND</button></p>
		
		<button type="button"  class="btn-mediumguest" onclick="jQuery('#soap-popupbox').css('display', 'none');">CLOSE</button>
                <div class="seperator"></div>
                
            </div>