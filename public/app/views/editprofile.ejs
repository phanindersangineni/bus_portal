<div class="page-title-container">
            <div class="container">
                <div class="page-title pull-left">
                    <h2 class="entry-title" style="font-size: 0.37em; !important" ng-bind="dashboardTittleLabel">My Dashboard</h2>
                </div>
                <ul class="breadcrumbs pull-right">
                    <li><a ng-click="nagivatehome()" ng-bind="homeLabel">HOME</a></li>
                    <li class="active" ng-bind="dashboardLabel">dashboard</li>
                </ul>
            </div>
        </div>


        <section id="content" class="gray-area">
            <div class="container">
                <div id=""><br>
                    <div class="tab-container full-width-style arrow-left dashboard">
                        <ul class="tabs">
                            <li class=""><a data-toggle="tab" href="#dashboard"><i class="soap-icon-anchor circle"></i><h6 ng-bind="dashboardLabel">Dashboard</h6></a></li>
                            <li class="active"><a data-toggle="tab" href="#profile"><i class="soap-icon-user circle"></i><h6 ng-bind="profileLabel">Profile</h6></a></li>
                            <li class=""><a data-toggle="tab" href="#mybooking"><i class="soap-icon-businessbag circle"></i><h6 ng-bind="mybookingLabel">My Booking</h6></a></li>
                          
                            <li class=""><a data-toggle="tab" href="#settings"><i class="soap-icon-settings circle"></i><h6 ng-bind="settingsLabel">Settings</h6></a></li>
                        </ul>
                        <div class="tab-content">
                           
                            <div id="profile" class="tab-pane fade in active">
                                
                                <div class="view-profile">
                                    <form class="edit-profile-form">
                                        <h2 ng-bind="personaldetailslabel">Personal Details </h2>
                                        <div class="col-sm-9 no-padding no-float">
                                            <div class="row form-group">
                                                <div class="col-sms-6 col-sm-6">
												 <label><span ng-bind="firstnameLabel">First Name</span><span style="color:red;margin-left:3px;font-size:18px;">*</span></label>
                                                 <!--   <label ng-bind="firstnameLabel">First Name</label>  -->
                                                    <input type="text" class="input-text full-width" placeholder="{{firstnameplaceholder}}" ng-model="firstname">
													<div id="firstnameErrorDiv"  style="width:200px;height:20px;color:red" hidden>
														<p  ng-bind="firstnameErrorMsg"></p>
														</div>
														
														<div id="firstnameErrorDiv2"  style="width:200px;height:20px;color:red" hidden>
														<p  ng-bind="firstnameErrorMsg2"></p>
														</div>
														
                                                </div>
                                                <div class="col-sms-6 col-sm-6">
												 <label><span ng-bind="lastnameLabel">Last Name</span><span style="color:red;margin-left:3px;font-size:18px;">*</span></label>
                                                <!--    <label ng-bind="lastnameLabel">Last Name</label>  -->
                                                    <input type="text" class="input-text full-width" placeholder="{{lastnameplaceholder}}" ng-model="lastname">
													<div id="lastnameErrorDiv"  style="width:200px;height:20px;color:red" hidden>
														<p  ng-bind="lastnameErrorMsg"></p>
														</div>
                                                </div>
                                            </div>
                                            <div class="row form-group">
                                                <div class="col-sms-6 col-sm-6">
												 <label><span ng-bind="emailLabel">Email</span><span style="color:red;margin-left:3px;font-size:18px;">*</span></label>
                                                  <!--  <label ng-bind="emailLabel">Email </label> -->
                                                    <input type="text" class="input-text full-width" placeholder="{{emailplaceholder}}" ng-model="email">
														<div id="emailErrorDiv"  style="width:200px;height:20px;color:red" hidden>
														<p  ng-bind="emailErrorMsg"></p>
														</div>
                                                </div>
												
												
												
												 <div class="col-sms-6 col-sm-6">
												  <label><span ng-bind="telLabel">Tel</span><span style="color:red;margin-left:3px;font-size:18px;">*</span></label>
                                                 <!--   <label ng-bind="telLabel">Tel</label>  -->
                                                    <input type="text" class="input-text full-width" placeholder="{{telplaceholder}}" ng-model="telephone" maxlength="15">
													<div id="telephoneErrorDiv"  style="width:200px;height:20px;color:red" hidden>
														<p  ng-bind="telephoneErrorMsg"></p>
														</div>
                                                </div>
                                                
                                            </div>
                                          
											<div class="row form-group">
											<div class="col-sms-6 col-sm-6">
										
                                                    <label ng-bind="languageLabel">Language</label>
                                                    <div class="">
                                                        <select class="full-width" ng-model="languageid" >
                                                            <option value="-1" ng-bind="selectlanguagelabel">Select language</option>
												<option data-ng-repeat="language in alllanguages" value="{{language.ID}}" ng-selected="languageid==language.ID" >{{language.NAME}}</option>  
                                                   
                                                       
													</select>
                                                    </div>
                                                </div>
												</div>
											
                                            <hr>
                                            <h2 ng-bind="contactdetailsLabel">Contact Details </h2>
                                            
                                            <div class="row form-group">
                                                
                                                <div class="col-sms-6 col-sm-6">
                                                    <label ng-bind="countryLabel">Country</label>
                                                    <div class="">
                                                        <select class="full-width" ng-model="countryid" ng-change="getcitiesbycountry(countryid)">
                                                            <option value="-1" ng-bind="selectcountrylabel">Select country</option>
															<option data-ng-repeat="country in allCountries" value="{{country.ID}}" >{{country.NAME}}</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-sms-6 col-sm-6">
                                                    <label ng-bind="cityLabel">City</label>
                                                    <div class="">
                                                        <select class="full-width" id="cityId" ng-model="cityid">
                                                            <option value="-1" ng-bind="selectcitylabel">Select city</option>
                                                            <option data-ng-repeat="city in allCities" value="{{city.ID}}" >{{city.NAME}}</option>
                                                        </select>
                                                    </div>
                                                </div>
                                            </div>
											<div class="row form-group">
                                                
                                                <div class="col-sms-6 col-sm-6">
												 <label><span ng-bind="addressLabel">Address</span><span style="color:red;margin-left:3px;font-size:18px;">*</span></label>
                                                 <!--   <label ng-bind="addressLabel">Address</label> -->
                                                    <input type="text" class="input-text full-width" ng-model="address" >
													<div id="addressErrorDiv"  style="width:200px;height:20px;color:red" hidden>
														<p  ng-bind="addressErrorMsg"></p>
														</div>
                                                </div>
												<div class="col-sms-6 col-sm-6">
                                                    <label ng-bind="companyLabel">Company</label>
                                                    <input type="text" class="input-text full-width" ng-model="company">
                                                </div>												
																							
                                            </div>                                    
												
										
                                            
                                            <div class="from-group">
                                                <button type="submit" class="btn-medium col-sms-6 col-sm-4" ng-click="updatebutton()"  ng-bind="updateprofileButton">UPDATE PROFILE</button>
                                            </div>

                                        </div>
                                    </form>
                                </div>
                            </div>
                            
                        </div>
                    </div>
                </div>
            </div><br>
        </section>


      